.mk-wrap {
    width: 100%;
    height: 98rpx;
    background-color: #fff;
    position: absolute;
    bottom: 0;
    border-top: 2px solid #DAD9E2;
    display: flex;
    justify-content: center;
    align-items: center;
}

.mk-wrap view {
    width: 690rpx;
    height: 70rpx;
    line-height: 70rpx;
    background-color: #EEE;
    text-align: center;
}

.mk-hover {
    background-color: #F5F5F5 !important;
}

.mk-hover text {
    color: #999 !important;
}

.mk-wrap view text {
    color: #444;
    font-size: 30rpx;
}

.mk-icon {
    line-height: 120rpx;
    font-size: 76rpx;
    color: #fff;
}

.mk-cancle {
    width:367rpx;
    height:384rpx;
    background: rgba(0, 0, 0, 0.7);
    border-radius: 26rpx;
    position: absolute;
    left: 50%;
    top: 50%;
    transform: translate(-50%, -50%);
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
}

.mk-cancle image {
    position: absolute;
}

.mk-cancle image:nth-child(1) {
    width: 91rpx;
    height: 135rpx;
    left: 81rpx;
    top: 104rpx;
}

.mk-cancle image:nth-child(2) {
    width: 81rpx;
    height: 121rpx;
    right: 80rpx;
    top: 119rpx;
}

.mk-cancle image:nth-child(3) {
    width: 36rpx;
    height: 32rpx;
    left: 107rpx;
    bottom: 33rpx;
}

.mk-cancle text {
    font-size: 24rpx;
    color: rgba(255, 255, 255, 0.4);
    position: absolute;
    left: 162rpx;
    bottom: 31rpx;
}